<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="HIS" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>ulb.lisa.his.model.Address</class>
    <class>ulb.lisa.his.model.Imagingstudy</class>
    <class>ulb.lisa.his.model.Series</class>
    <class>ulb.lisa.his.model.User</class>
    <class>ulb.lisa.his.model.Practitioner</class>
    <class>ulb.lisa.his.model.Person</class>
    <class>ulb.lisa.his.model.Patient</class>
    <class>ulb.lisa.his.model.Practitionerdicomidentifier</class>
    <class>ulb.lisa.his.model.Patientdicomidentifier</class>
    <class>ulb.lisa.his.model.Instance</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/his?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
